{
  "rewrites": [
    {
      "source": "/blog/:subdir*",
      "destination": "https://hugo-blog-vert-one.vercel.app/:subdir*"
    },
    {
      "source": "/blog/:subdir*/",
      "destination": "https://hugo-blog-vert-one.vercel.app/:subdir*/"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
